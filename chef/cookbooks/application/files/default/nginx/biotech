server {
    server_name  biotech.at *.biotech.at;
    access_log  /var/log/nginx/biotech.log;

    location / {
        ssi on;
        proxy_pass http://127.0.0.1:8888;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }

    location ^~ /media/ {
        root /var/www/biotech.at;
    }

    location ^~ /static/ {
        root /var/www/biotech.at;
    }
}

server {
    server_name media.biotech.at;
    access_log  /var/log/nginx/biotech.log;

    location / {
        root /var/www/biotech.at;
    }
}
